<mvc:View
controllerName="HNGv2.highnoongamble.controller.Readings"
xmlns="sap.m"
xmlns:mvc="sap.ui.core.mvc"
displayBlock="true"
xmlns:layout="sap.ui.layout">
        <layout:VerticalLayout class="sapUiResponsiveMargin">
            <Title titleStyle="H2" text="{i18n>readingsGroupTitle}" class="sapUiTinyMarginBegin, titleText" />
            <layout:HorizontalLayout allowWrapping="true">
                <GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout preambleTile" press=".onNavToPreamble" url="https://www.sap.com/">
