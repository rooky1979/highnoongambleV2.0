                        <ImageContent src="webapp/images/mindout.jpg" />
                    </TileContent>
                </GenericTile>
                <GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout chapter3Tile"  press=".onNavToChapter3" url="https://www.sap.com/" >
                    <TileContent>
                        <ImageContent src="webapp/images/mindout.jpg" />
                    </TileContent>
                </GenericTile>
                <GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout chapter5Tile"  press=".onNavToChapter5" url="https://www.sap.com/">
                    <TileContent>
                        <ImageContent src="webapp/images/mindout.jpg" />
                    </TileContent>
                </GenericTile>
                <GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout traditionsTile" press=".onNavToTraditions" url="https://www.sap.com/">
                    <TileContent>
                        <ImageContent src="webapp/images/mindout.jpg" />
                    </TileContent>
                </GenericTile>
                <GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout responsibilityTile" press=".onNavToResponsibility" url="https://www.sap.com/">
                    <TileContent>
                        <ImageContent src="webapp/images/mindout.jpg" />
                    </TileContent>
                </GenericTile>
            </layout:HorizontalLayout>
        </layout:VerticalLayout>
</mvc:View>