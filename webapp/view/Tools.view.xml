<mvc:View
controllerName="HNGv2.highnoongamble.controller.Tools"
xmlns="sap.m"
xmlns:mvc="sap.ui.core.mvc"
displayBlock="true"
xmlns:layout="sap.ui.layout">
        <layout:VerticalLayout class="sapUiResponsiveMargin">
            <Title titleStyle="H2" text="{i18n>toolsGroupTitle}" class="sapUiTinyMarginBegin" />
            <layout:HorizontalLayout allowWrapping="true">
                <GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" header="{i18n>myMindTitle}" subheader="{i18n>myMindTagline}" press=".onNavToMyMind">
                    <TileContent>
                    </TileContent>
                </GenericTile>
                <GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" header="{i18n>commonClichesTitle}" subheader="{i18n>commonClichesTagline}" press=".onNavToCommonCliches">
                    <TileContent>
                        <ImageContent src="webapp/images/mindout.jpg" />
                    </TileContent>
                </GenericTile>
                <GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" header="{i18n>additionalClichesTitle}" subheader="{i18n>additionalClichesTagline}" press=".onNavToAdditionalCliches">
                    <TileContent>
                        <ImageContent src="webapp/images/mindout.jpg" />
                    </TileContent>
                </GenericTile>
                <GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" header="{i18n>soberCardsTitle}" subheader="{i18n>soberCardsTagline}" press=".onNavToSoberCards">
                    <TileContent>
                        <ImageContent src="webapp/images/mindout.jpg" />
                    </TileContent>
                </GenericTile>
                <GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" header="{i18n>bigbookCardsTitle}" subheader="{i18n>bigbookCardsTagline}" press=".onNavToBBCards">
                    <TileContent>
                        <ImageContent src="webapp/images/mindout.jpg" />
                    </TileContent>
                </GenericTile>
            </layout:HorizontalLayout>
        </layout:VerticalLayout>
</mvc:View>