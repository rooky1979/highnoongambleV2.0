<mvc:View
controllerName="HNGv2.highnoongamble.controller.Tools"
xmlns="sap.m"
xmlns:mvc="sap.ui.core.mvc"
displayBlock="true"
xmlns:layout="sap.ui.layout">
        <layout:VerticalLayout class="sapUiResponsiveMargin">
            <Title titleStyle="H2" text="{i18n>toolsGroupTitle}" class="sapUiTinyMarginBegin, titleText" />
            <layout:HorizontalLayout allowWrapping="true">
                <GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout mymindTile"  press=".onNavToMyMind">
                    <TileContent>
                    </TileContent>
                </GenericTile>
                <GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout commonClichesTile"  press=".onNavToCommonCliches">
                    <TileContent>
                        <ImageContent src="webapp/images/mindout.jpg" />
                    </TileContent>
                </GenericTile>
                <GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout additionalClichesTile" press=".onNavToAdditionalCliches">
                    <TileContent>
                        <ImageContent src="webapp/images/mindout.jpg" />
                    </TileContent>
                </GenericTile>
                <GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout soberCardsTile"  press=".onNavToSoberCards">
                    <TileContent>
                        <ImageContent src="webapp/images/mindout.jpg" />
                    </TileContent>
                </GenericTile>
                <GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout bbCardsTile"  press=".onNavToBBCards">
                    <TileContent>
                        <ImageContent src="webapp/images/mindout.jpg" />
                    </TileContent>
                </GenericTile>
                <GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout godShotTile" press=".onNavToGodShot">
                    <TileContent>
                        <ImageContent src="webapp/images/mindout.jpg" />
                    </TileContent>
                </GenericTile>
            </layout:HorizontalLayout>
        </layout:VerticalLayout>
</mvc:View>